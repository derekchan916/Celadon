<li>
  <div class="review-header">
    <p>
      By <%= review.escape('fname') %> <%= review.escape('lname') %> on <%= review.escape('date') %>
    </p>
    <p class='title'>
      <div class="user-star-rating" id="user-star-rating"></div>
      <p class='bold-title'><%= review.escape('title') %></p>
    </p>
  </div>
  <div class="review-body">
    <p>
      <%= review.escape('body') %>
    </p>
  </div>
</li>
